class Human {
	private String name;
	private int age;	
	public Human setName(String name) {
		this.name = name;
		return this;
	}
	public Human setAge(int age) {
		this.age = age;
		return this;
	}
	@Override
	public String toString() {
		return name + "(" + age + ")";
	}
}

